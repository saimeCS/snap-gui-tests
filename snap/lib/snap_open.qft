                </SetGlobalStep>
              </ElseSequence>
            </IfSequence>
            <ElseSequence id="_1rMT">
              <SetGlobalStep id="_1sKy" varname="input_file">
                <default>$(data_source_directory)$(prod)</default>
              </SetGlobalStep>
            </ElseSequence>
